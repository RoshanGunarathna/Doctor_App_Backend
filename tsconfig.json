{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "target": "ES2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
   //  "strict": true,
    "skipLibCheck": true,
  }
}
